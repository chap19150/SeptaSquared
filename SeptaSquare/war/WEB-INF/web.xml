<?xml version="1.0" encoding="utf-8" standalone="no"?><web-app xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="2.5" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
	<servlet>
		<servlet-name>GetHereNow</servlet-name>
		<servlet-class>com.chapslife.septasquare.push.GetHereNow</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>PushHandler</servlet-name>
		<servlet-class>com.chapslife.septasquare.push.PushHandler</servlet-class>
	</servlet>
	<servlet>
    <servlet-name>ChannelHandler</servlet-name>
    <servlet-class>com.chapslife.septasquare.push.ChannelHandler</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>IndexServlet</servlet-name>
    <servlet-class>com.chapslife.septasquare.push.IndexServlet</servlet-class>
  </servlet>
	<servlet-mapping>
		<servlet-name>GetHereNow</servlet-name>
		<url-pattern>/herenow</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>PushHandler</servlet-name>
		<url-pattern>/handlepush</url-pattern>
	</servlet-mapping>
  <servlet-mapping>
    <servlet-name>ChannelHandler</servlet-name>
    <url-pattern>/_ah/channel/*</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>IndexServlet</servlet-name>
    <url-pattern>/*</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>SystemServiceServlet</servlet-name>
    <servlet-class>com.google.api.server.spi.SystemServiceServlet</servlet-class>
    <init-param>
      <param-name>services</param-name>
      <param-value/>
    </init-param>
  </servlet>

  <servlet-mapping>
    <servlet-name>SystemServiceServlet</servlet-name>
    <url-pattern>/_ah/spi/*</url-pattern>
  </servlet-mapping>

</web-app>